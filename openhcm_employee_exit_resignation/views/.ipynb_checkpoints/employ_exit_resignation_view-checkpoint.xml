<?xml version="1.0" encoding="utf-8"?><odoo>		<record id="view_exit_request_form" model="ir.ui.view">			<field name="name">hr.employee.exit.resignation.form</field>			<field name="model">hr.employee.exit.resignation</field>			<field name="arch" type="xml">				<form string="hr.employee.exit.resignation">					<header>						<button name="action_browse" type="object" string="Confirm"  class="oe_highlight"								states="draft"/>						<button name="approved_by_dept_manager" type="object" string="Approved by Dept. Manager"  class="oe_highlight"								states="Confirm"								groups="openhcm_employee_exit_resignation.dept_manager_access"						/>						<button name="approved_by_hr_manager" type="object" string="Approved by Hr Manager"  class="oe_highlight"								states="Approved by Dept. Manager"								groups="openhcm_employee_exit_resignation.hr_manager_access"						/>						<button name="approve_by_general_manager" type="object" string="Approved by General Manager"  class="oe_highlight"								states="Approved by Hr Manager"								groups="openhcm_employee_exit_resignation.general_manager_access"						/>						<button name= "cancel_action"								type="object"								string="Cancel"								class="oe_highlight"								state="Confirm,Approved by Dept. Manager,Approved by Hr Manager,Approved by General Manager"						/>						<field name="state"							   widget="statusbar"							   statusbar_visible="draft,							   Confirm,							   Approved by Dept. Manager,							   Approved by Hr Manager,							   Approved by General Manager,							   cancel"						/>					</header>					<sheet>                    <div class="oe_title">                      <h1>                          <field name="name" placeholder="Employee's Name" required="True"/>                      </h1><!--						<div name="button_box" class="oe_right oe_button_box">--><!--							<button  type="object"  icon="fa-user">--><!--								<field string="Start Interview" name="start_interview" widget="statinfo"/>--><!--							</button>--><!--							<button  type="object"  icon="fa-printer">--><!--								<field string="Print Interview" name="print_Interview" widget="statinfo"/>--><!--							</button>--><!--							<button  type="object"  icon="fa-calender">--><!--								<field string="Meeting" name="meeting" widget="statinfo"/>--><!--							</button>--><!--						</div>-->					</div>                    <group>							<group>								<field name="request_Date" />								<field name="last_day_of_work" />                                <field name="contact" />							</group>                            <group>                                <field name="department_id"/>                                <field name="job_id"/>                                <field name="parent_id"/>                                <field name="user_id"/>                                <field name="interview_id"/>                            </group>                    </group>                    <group>                        <group>                            <field name="Confirm_by" />							<field name="confirm_date"/>                            <field name="Department_manager"/>							<field name="department_manager_approve_date"/>                        </group>                        <group>                            <field name="Hr_manager" />                            <field name="hr_manger_approved_date"/>							<field name="General_manager" />                            <field name="general_manager_approved_date"/>                        </group>                    </group>                    <notebook>							<page string ='Information'>								<field name = 'employ_information'>								<tree editable="bottom">									<field name="Check_list" />									<field name="Responsible_User" />									<field name="Remarks"/>									<field name="State"/>									<field name="Information" invisible = '1'/>								</tree>								<form>									<group>										<group>											<field name="Check_list" />											<field name="Responsible_User" />										</group>										<group>											<field name="Remarks"/>											<field name="State"/>										</group>									</group>								</form>								</field>							</page>                            <page string ='Contact'>								<field name = 'employ_information'>								<tree editable="bottom">									<field name="Check_list" />									<field name="Responsible_User" />									<field name="Remarks"/>									<field name="State"/>									<field name="Information" invisible = '1'/>								</tree>								<form>									<group>										<group>											<field name="Check_list" />											<field name="Responsible_User" />										</group>										<group>											<field name="Remarks"/>											<field name="State"/>										</group>									</group>								</form>								</field>							</page>						</notebook>					<group>						<field name="reason_for_exit"/>						<field name="notes"/>					</group>					</sheet>
                    <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>				</form>			</field>		</record>		<record id="exit_request_tree" model="ir.ui.view">			<field name="name">hr.employee.exit.resignation.tree</field>			<field name="model">hr.employee.exit.resignation</field>			<field name="arch" type="xml">				<tree string="hr.employee.exit.resignation Tree">                    <field name="name" />                    <field name="request_Date" />                    <field name="last_day_of_work" />				</tree>			</field>		</record>		<record id="exit_request_search" model="ir.ui.view">			<field name="name">hr.employee.exit.resignation.search</field>			<field name="model">hr.employee.exit.resignation</field>			<field name="arch" type="xml">				<search string="hr.employee.exit.resignation search">				</search>			</field>		</record>		<record id="action_exit_request_root_view" model="ir.actions.act_window">            <field name="name">Exit Request</field>            <field name="res_model">hr.employee.exit.resignation</field>            <field name="type">ir.actions.act_window</field>            <field name="view_mode">tree,form</field>            <field name="domain">[]</field>            <field name="context">{}</field>            <field name="search_view_id" ref="exit_request_search"/>            <field name="view_id" ref="exit_request_tree"/>            <field name="help" type="html">                <p class="o_view_nocontent_smiling_face">                   hr.employee.exit.resignation root menu action                </p>            </field>        </record>		<record id="action_exit_form" model="ir.actions.act_window">            <field name="name">check_list</field>            <field name="res_model">hr.employee.exit.resignation</field>            <field name="type">ir.actions.act_window</field>            <field name="view_mode">form</field>            <field name="domain">[]</field>            <field name="context">{}</field>            <field name="search_view_id" ref="exit_request_search"/>            <field name="view_id" ref="view_exit_request_form"/>            <field name="help" type="html">                <p class="o_view_nocontent_smiling_face">                   hr.employee.exit.resignation check list action                </p>            </field>        </record>		<record id="view_Check_list_form" model="ir.ui.view">			<field name="name">hr.employee.exit.resignation.notebook.form</field>			<field name="model">hr.employee.exit.resignation.notebook</field>			<field name="arch" type="xml">				<form string="hr.employee.exit.resignation">					<sheet>						<group>							<group>								<field name="Check_list" />								<field name="Responsible_User" />							</group>							<group>								<field name="Remarks"/>								<field name="State"/>							</group>						</group>					</sheet>				</form>			</field>		</record>	<record id="check_list_tree" model="ir.ui.view">			<field name="name">hr.employee.exit.resignation.notebook.tree</field>			<field name="model">hr.employee.exit.resignation.notebook</field>			<field name="arch" type="xml">				<tree string="hr.employee.exit.resignation Tree">                    <field name="Check_list" />                    <field name="Responsible_User" />                    <field name="Remarks" />                    <field name="State" />				</tree>			</field>		</record>	<record id="check_list_search" model="ir.ui.view">			<field name="name">hr.employee.exit.resignation.notebook.search</field>			<field name="model">hr.employee.exit.resignation.notebook</field>			<field name="arch" type="xml">				<search string="hr.employee.exit.resignation search">				</search>			</field>		</record>	<record id="action_check_list" model="ir.actions.act_window">            <field name="name">check_list</field>            <field name="res_model">hr.employee.exit.resignation.notebook</field>            <field name="type">ir.actions.act_window</field>            <field name="view_mode">tree,form</field>            <field name="domain">[]</field>            <field name="context">{}</field>            <field name="search_view_id" ref="check_list_search"/>            <field name="view_id" ref="check_list_tree"/>            <field name="help" type="html">                <p class="o_view_nocontent_smiling_face">                   hr.employee.exit.resignation check list action                </p>            </field>        </record></odoo>