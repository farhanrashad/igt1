<odoo>
  <data>
    <record id="hr_employee_form_view" model="ir.ui.view">
			<field name="name">hr.employee.form.inherit</field>
			<field name="model">hr.employee</field>
              <field name="inherit_id" ref="hr.view_employee_form"/>
			<field name="arch" type="xml">
				<xpath expr="//page[@name='hr_settings']" position='after'>
				 <page string="Work Experience">
					 <field name="experience_lines">
							<tree>
								<field name="company"/>
							<field name="employee_id"/>
							<field name="job_title"/>
							<field name="From"/>
							<field name="To"/>
							<field name="comments"/>
							<field name="experience_id"/>
							</tree>
					 </field>
					<!--	relation id  -->
					<field name="experience_lines">
						<tree editable="bottom">
							<control>
								<create name="add_line_control" string="Add a line"/>
							</control>
					hr		<field name="company"/>
							<field name="employee_id"/>
							<field name="job_title"/>
							<field name="From"/>
							<field name="To"/>
							<field name="comments"/>
							<field name="experience_id"/>
						</tree>
					</field>
				 </page>

                  <page string="Employee skill">
					<!--	relation id  -->
					<field name="skill_lines">
						<tree editable="bottom">
							<field name="skill"/>
							<field name="years_of_experience"/>
						</tree>
					</field>
				 </page>
                  <page string="Employee Language">
					<!--	relation id  -->
					<field name="language_lines">
						<tree editable="bottom">
							<field name="language"/>
							<field name="fluency"/>
							<field name="competency"/>
							<field name="comments"/>
						</tree>
					</field>
				 </page>
				<page string="Employee Education">
					<!--	relation id  -->
					<field name="education_lines">
						<tree editable="bottom">
							<field name="level"/>
							<field name="year"/>
							<field name="cgpa_score"/>
						</tree>
					</field>
				</page>
				<page string="Employee License">
					<!--	relation id  -->
					<field name="experience_lines">
						<tree editable="bottom">
							<field name="company"/>
						</tree>
					</field>
				 </page>
<!--					<page string="Employee Family">-->
<!--					&lt;!&ndash;	relation id  &ndash;&gt;-->
<!--					<field name="experience_lines">-->
<!--						<tree editable="bottom">-->
<!--							<field name="company"/>-->

<!--						</tree>-->
<!--					</field>-->
<!--				 </page>-->
				<page string="Employee attachment">
					<!--	relation id  -->
					<field name="attachment_lines">
						<tree editable="bottom">
							<field name="attachment"/>
						</tree>
					</field>
				 </page>
				</xpath>
			</field>
		</record>
