<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!--		form view for visit duration-->
		<record id="view_visitor_duration_form" model="ir.ui.view">
			<field name="name">visit.duration.form</field>
			<field name="model">visit.duration</field>
			<field name="arch" type="xml">
				<form string="Visitor Duration">
					<sheet>
						<h1>Visitor Duration</h1>
						<group>
							<field name="name"/>
							<field name="dur_code"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>


		<record id="view_visitor_duration_tree" model="ir.ui.view">
			<field name="name">visit.duration.tree</field>
			<field name="model">visit.duration</field>
			<field name="arch" type="xml">
				<tree string="Visit Duration Tree">
					<field name="name" />
					<field name="dur_code" />
				</tree>
			</field>
		</record>

		<record id="view_visitor_duration_search" model="ir.ui.view">
			<field name="name">visit.duration.search</field>
			<field name="model">visit.duration</field>
			<field name="arch" type="xml">
				<search string="Visit Duration search">
					<field name="name" />
					<field name="dur_code" />
				</search>
			</field>
		</record>


		<record id="action_visit_duration_view" model="ir.actions.act_window">
            <field name="name">Visitor Duration</field>
            <field name="res_model">visit.duration</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="search_view_id" ref="view_visitor_duration_search"/>
            <field name="view_id" ref="view_visitor_duration_tree"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                   Visit Duration
                </p>
            </field>
        </record>
</odoo>


