<?xml version="1.0" encoding="utf-8"?>
<odoo>


         <record id="visitor_calender" model="ir.ui.view">
            <field name="name">Visitor Calendar</field>
            <field name="model">visit.detail</field>
            <field name="arch" type="xml">
                <calendar string="Calendar" date_start="check_out">
                    <field name="check_in"/>
                </calendar>
            </field>
        </record>
        <record id="view_visitor_pivot" model="ir.ui.view">
                 <field name="name">visitor.report.pivot</field>
                 <field name="model">visit.detail</field>
                 <field name="arch" type="xml">
                     <pivot string="Visitor Reprot" disable_linking="True">
                         <field name="employee_id" type="row"/>
                         <field name="check_out" type="col"/>
<!--                         <field name="team_id" type="col"/>-->
<!--                         <field name="date" interval="month" type="row"/>-->
<!--                         <field name="price_subtotal" type="measure"/>-->
                     </pivot>
                 </field>
            </record>

            <record id="view_report_graph" model="ir.ui.view">
                 <field name="name">visitor.report.graph</field>
                 <field name="model">visit.detail</field>
                 <field name="arch" type="xml">
                     <graph string="Visitor Analysis" type="line">
                         <field name="check_in" type="measure"/>
                         <field name="check_out" type="measure"/>
                     </graph>
                 </field>
            </record>




